{
  "PokerLab": {
    "location": "chap01/PokerLab",
    "lab name": "C Programming Familiarization Lab",
    "required files": "your screenshot, \\textit{card.c}, \\textit{poker.c}, and \\textit{answers.txt}"
  },
  "KeyboardLab": {
    "location": "chap02/KeyboardLab",
    "lab name": "Bit Manipulation Lab",
    "required files": "\\textit{problem1.c}, \\textit{problem2.c}, and \\textit{problem3.c}",
    "compilation": {
      "C": {
        "warnings": "-Wall -Wextra -Wno-unused-parameter -Wno-unused-value",
        "libraries": "-lm"
      }
    },
    "lab-specific keys": [
      "first missing key",
      "desired email",
      "adhesive"
    ]
  },
  "LinkedListLab": {
    "location": "chap01/LinkedListLab",
    "lab name": "Pointer Manipulation Lab",
    "required files": "\\textit{answers.txt}, \\textit{challenge-response.c}, \\textit{linked\\_list.c}, and \\textit{word\\_entry.c}",
    "compilation": {
      "C": {
        "custom targets": [
          {
            "target": "arraylistlab",
            "dependencies": "$(filter-out linked_list.o,$(OBJ))"
          },
          {
            "target": "linkedlistlab",
            "dependencies": "$(filter-out array_list.o,$(OBJ))"
          }
        ]
      }
    }
  },
  "IntegerLab": {
    "location": "chap03/IntegerLab",
    "lab name": "Integer Representation and Arithmetic Lab",
    "required files": "\\textit{basetwo.c} and \\textit{alu.c}",
    "compilation": {
      "C": {
        "optimization": "-Og -g -finstrument-functions",
        "libraries": "-lm"
      },
      "asm": {
        "suffix": ".asm"
      }
    }
  },
  "FloatLab": {
    "location": "chap04/FloatLab",
    "lab name": "Floating Point Representation and Arithmetic Lab",
    "required files": "\\textit{fpu.c}",
    "compilation": {
      "C": {
        "warnings": "-Wall -Wextra -Wno-unused-variable -Wno-unused-parameter"
      }
    }
  }
}